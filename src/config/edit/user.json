{
  "name": "Edit user",
  "inMenu": false,
  "url": "/user/edit/:id",
  "page": {
    "type": "edit",
    "options": {
      "initial": "https://jsonplaceholder.typicode.com/users",
      "actions": {
        "save": {
          "type": "call",
          "options": {
            "url": "https://jsonplaceholder.typicode.com/users",
            "method": "PUT",
            "headers": {
              "Content-type": "application/json; charset=UTF-8"
            }
          }
        },
        "reset": {
          "type": "reset"
        }
      },
      "fields": [
        {
          "name": "name",
          "type": "string"
        },
        {
          "name": "username",
          "type": "string"
        },
        {
          "name": "email",
          "type": "string"
        },
        {
          "name": "phone",
          "type": "string"
        },
        {
          "name": "address",
          "type": "Address"
        }
      ]
    }
  }
}
